{
    "name": "laravel/socioboard5.0",
    "type": "project",
    "description": "The Laravel Framework for Socioboard5.0",
    "keywords": [
        "Framework",
        "Laravel",
        "^8.0",
        "Modules",
        "Social",
        "neomorphic design"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.12",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.2",
        "nwidart/laravel-modules": "^8.2",
        "php-http/curl-client": "^2.2",
        "ext-json": "*"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "app/Classes"
        ],
        "files": [
            "app/Classes/AuthUsers.php",
            "app/AuthUsers/main.php",
            "app/ApiConfig/ApiConfig.php",
            "app/ApiConfig/config.php"
        ],
        "exclude-from-classmap": ["./Modules/Discovery/Http/Requests/searchYoutube.php", "./Modules/User/Database/Seeders/UserDatabaseSeeder.php", "./Modules/User/Providers/RouteServiceProvider.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
